{
  "compilerOptions": {
    "strictNullChecks": true,
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "CommonJS",
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowJs": true,
    "esModuleInterop": true
  }
}
